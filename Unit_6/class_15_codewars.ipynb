    "def digital_root(n):\n",
    "    test = int(str(n)[::]) \n",
    "    while len(str(test)) > 1:\n",
    "        total = 0\n",
    "        print(test)\n",
    "        for item in str(test):\n",
    "            total += int(item)\n",
    "            test = total\n",
    "    return test\n",
    "\n",
    "\n",
    "\n",
    "print(digital_root(456))"
   ]
